}

/* Link Styles - NOS Theme */
a {
  color: #E20714;
  text-decoration: none;
  transition: all 0.3s ease;
}

a:hover {
  color: #C10612;
  text-decoration: none;
}

/* Heading Styles - NOS Theme */
h1, h2, h3, h4, h5, h6 {
  color: #333333;
  font-weight: 700;
}

h1 {
  font-size: 2.5rem;
  color: #E20714;
  letter-spacing: -1px;
}

h2 {
  font-size: 2rem;
  color: #333333;
}

h3 {
  font-size: 1.5rem;
  color: #333333;
}

/* Modal Styles - NOS Theme */
.modal-content {
  border-radius: 0;
  border: none;
  box-shadow: 0 4px 24px rgba(0,0,0,0.2);
}

.modal-header {
  background: #E20714;
  color: white;
  border-bottom: none;
  padding: 1.5rem;
}

.modal-title {
  font-weight: 700;
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

.modal-footer {
  border-top: 1px solid #E5E5E5;
  padding: 1rem 1.5rem;
}

.btn-close {
  filter: brightness(0) invert(1);
}

/* Form Styles - NOS Theme */
.form-control {
  border-radius: 0;
  border: 2px solid #E5E5E5;
  padding: 0.75rem;
  transition: all 0.3s ease;
}

.form-control:focus {
  border-color: #E20714;
  box-shadow: 0 0 0 0.2rem rgba(226, 7, 20, 0.15);
}

.form-label {
  font-weight: 600;
  color: #333333;
  text-transform: uppercase;
  font-size: 0.85rem;
  letter-spacing: 0.5px;
  margin-bottom: 0.5rem;
}

.form-select {
  border-radius: 0;
  border: 2px solid #E5E5E5;
  padding: 0.75rem;
}

.form-select:focus {
  border-color: #E20714;
  box-shadow: 0 0 0 0.2rem rgba(226, 7, 20, 0.15);
}

/* Responsive adjustments - NOS Theme */
@media (max-width: 768px) {
  .component-container {
    padding: 1.5rem;
  }
  
  .table {
    font-size: 0.875rem;
  }
  
  .navbar {
    padding: 1rem;
  }
  
  .navbar-logo {
    height: 35px;
  }
  
  h1 {
    font-size: 2rem;
  }
  
  .jumbotron {
    padding: 2.5rem 1.5rem;
  }
  
  .btn {
    padding: 0.6rem 1.5rem;
    font-size: 0.85rem;
  }
}
