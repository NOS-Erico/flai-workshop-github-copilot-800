/* Component Container - NOS Theme */
.component-container {
  background: #FFFFFF;
  border-radius: 0;
  padding: 2.5rem;
  box-shadow: 0 2px 12px rgba(0,0,0,0.06);
  margin-bottom: 2rem;Rainbow Theme */
.badge {
  padding: 0.5rem 1rem;
  font-weight: 700;
  border-radius: 20px;
  text-transform: uppercase;
  letter-spacing: 0.5px;
  font-size: 0.75rem;
  box-shadow: 2px 2px 0px rgba(0,0,0,0.1);
}

.badge-primary {
  background: linear-gradient(135deg, var(--nos-purple) 0%, var(--nos-pink) 100%);
  color: white;
}

.badge.bg-primary {
  background: linear-gradient(135deg, var(--nos-cyan) 0%, var(--nos-blue) 100%) !important;
}

.badge-success, .badge.bg-success {
  background: linear-gradient(135deg, var(--nos-green) 0%, var(--nos-teal) 100%);
  color: white;
}

.badge-warning, .badge.bg-warning {
  background: linear-gradient(135deg, var(--nos-yellow) 0%, var(--nos-orange) 100%);
  color: #333333;
}

.badge-danger, .badge.bg-danger {
  background: linear-gradient(135deg, var(--nos-red) 0%, var(--nos-pink) 100%);
  color: white;
}

.badge.bg-info {
  background: linear-gradient(135deg, var(--nos-cyan) 0%, var(--nos-blue) 100%)
  transition: all 0.2s ease;
  border-bottom: 1px solid #E5E5E5;
}

.table tbody tr:nth-child(even) {
  background-color: #FAFAFA;
}

.table tbody tr:hover {
  background: #FFF0F0;
  transform: none;
  box-shadow: none;
  border-left: 3px solid #E20714;
}

.table tbody td {
  vertical-align: middle;
  padding: 1rem;
  color: #333333;
  font-weight: 500;
}

/* Loading Spinner - NOS Theme */
.spinner-container {
